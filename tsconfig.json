{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "paths": {
      "assets/*": ["assets/*"]
    },
    "typeRoots": ["node_modules/@types", "src/types"],
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
